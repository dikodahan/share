    "version": "0.2.0",
    "configurations": [
        {
            "name": "Start",
            "program": "${workspaceFolder}/lib/backend/index.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node",
            "envFile": "${workspaceFolder}/local.env"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Build Channels",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/lib/backend/build-channels.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}